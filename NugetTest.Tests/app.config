	</system.serviceModel>
	<system.web>
		<webServices>
			<soapExtensionTypes>
				<add group="Low" priority="1" type="NugetTest.Tests.SoapLogger, NugetTest.Tests" />
			</soapExtensionTypes>
		</webServices>
	</system.web>
