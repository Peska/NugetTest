<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.serviceModel>
		<bindings>
			<basicHttpsBinding>
				<binding name="QueryPESELSoap11Binding" />
			</basicHttpsBinding>
			<customBinding>
				<binding name="QueryPESELSoap12Binding">
					<textMessageEncoding messageVersion="Soap12" />
